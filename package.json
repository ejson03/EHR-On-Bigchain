{
    "name": "ehr",
    "version": "1.0.0",
    "description": "ehr using bigchaindb",
    "main": "main.js",
    "homepage": "./",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "nodemon server.js",
        "deploy": "node server.js",
        "electron-start": "electron .",
        "package": "npm run -s package-win && npm run -s package-cleanup",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
        "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32  --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Music Player\"",
        "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64  --prune=true --out=release-builds",
        "package-cleanup": "rimraf build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ejson03/EHR.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ejson03/EHR/issues"
    },
    "dependencies": {
        "bigchaindb-driver": "^4.1.2",
        "connect-flash": "^0.1.1",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^8.2.0",
        "easy-bigchain": "^1.0.1",
        "ejs": "^3.1.3",
        "ejs-lint": "^1.1.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "formidable": "^1.2.2",
        "ipfs-api": "^26.1.2",
        "moment": "^2.27.0",
        "mongodb": "^3.5.9",
        "node-fetch": "^2.6.0",
        "node-rsa": "^1.1.1",
        "node-vault-user-pass": "0.0.5",
        "nodemailer": "^6.4.10",
        "open": "^7.1.0",
        "ps-tree": "^1.2.0",
        "uuid": "^8.2.0",
        "webpack": "^4.43.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.4",
        "electron": "^9.1.1",
        "electron-builder": "^22.7.0",
        "electron-packager": "^15.0.0",
        "rimraf": "^3.0.2"
    }
}
